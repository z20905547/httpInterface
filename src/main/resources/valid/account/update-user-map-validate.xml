<?xml version="1.0" encoding="UTF-8"?>

<validate-mapper name="update-user">

    <key name="email" alias="手机号码">
        <notEmpty />
        <length max="11" />
    </key>

    <key name="nickname" alias="昵称">
        <notEmpty />
        <length max="16" />
    </key>

    <key name="state" alias="状态">
        <notEmpty />
        <number />
    </key>

</validate-mapper>
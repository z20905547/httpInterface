<?xml version="1.0" encoding="UTF-8"?>

<validate-mapper name="insert-user">

    <key name="username" alias="登录帐号">
        <notEmpty />
        <length min="6" max="16" />
    </key>

    <key name="email" alias="电子邮件">
        <email/>
        <length max="64" />
    </key>

    <key name="password" alias="登录密码">
        <notEmpty />
        <length min="6" max="32" />
    </key>

    <key name="nickname" alias="昵称">
        <notEmpty />
        <length max="16" />
    </key>

    <key name="state" alias="状态">
        <notEmpty />
        <number />
    </key>

</validate-mapper>
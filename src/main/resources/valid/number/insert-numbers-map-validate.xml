<?xml version="1.0" encoding="UTF-8"?>

<validate-mapper name="insert-numbers">

    <key name="kname" alias="用户姓名">
        <notEmpty />
        <length min="1" max="50" />
    </key>

    <key name="ksex" alias="用户性别">
         <notEmpty /> 
    </key>
    <key name="knum" alias="客户手机号">
                <notEmpty />
        <length min="1" max="20" />
       
    </key>
 
</validate-mapper>